<h1>Listing playedgames</h1>

<table>
  <thead>
    <tr>
      <th>Player</th>
      <th>Game</th>
      <th>Playedtime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @playedgames.each do |playedgame| %>
      <tr>
        <td><%= playedgame.player_id %></td>
        <td><%= playedgame.game_id %></td>
        <td><%= playedgame.playedtime %></td>
        <td><%= link_to 'Show', playedgame %></td>
        <td><%= link_to 'Edit', edit_playedgame_path(playedgame) %></td>
        <td><%= link_to 'Destroy', playedgame, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Playedgame', new_playedgame_path %>
